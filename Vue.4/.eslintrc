{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "strict": 1,
    "no-use-before-define": [
      "error",
      {
        "functions": false,
        "classes": true
      }
    ],
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-undef": 0,
    "class-methods-use-this": 0
  },
  "extends": "airbnb/base",
  "globals": {}
}
